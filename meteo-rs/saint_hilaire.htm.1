    function GetObjectFromNameCustom( nom )
    {
        var Pdiv = undefined;

        if ( document.getElementById && document.getElementById( nom ))
        {
            Pdiv = document.getElementById( nom );        
        }    
        else if ( document.all && document.all[ nom ] )
        {
            Pdiv = document.all[ nom ];
